<odoo>
    <data>
        <record id="metal_product_profile_type_list_view" model="ir.ui.view">
            <field name="name">metal.profile.type.list.view</field>
            <field name="model">metal.profile.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" />
                </tree>
            </field>
        </record>
        

        <record id="metal_profile_type_action" model="ir.actions.act_window">
            <field name="name">Profile Type</field>
            <field name="res_model">metal.profile.type</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>