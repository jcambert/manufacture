<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="metal_quotation_material_template_view_tree" model="ir.ui.view">
        <field name="name">metal.quotation.material.template.view.tree</field>
        <field name="model">metal.quotation.material.template</field>
        <field name="arch" type="xml">
            <tree string="Material Template">
                <field name="sequence" widget="handle" />
                <field name="base" />
                <field name="name"/>
                <field name="volumic_mass_id"/>
                <field name="material_ids"/>
                <field name="price"/>
                <field name="create_date"/>
                <field name="create_uid"/>
                <field name="write_date"/>
                <field name="write_uid"/>
            </tree>
        </field>
    </record>

</odoo>
